        var suburbLocation = "";
        function initializeSuburbFinderWidget() {
        var addressFinderInstance = new AddressFinder.Widget(
        document.getElementById('ab_propertyname-1728354172346'),
        'QGWLATPDXYUEV6983MKJ',
        'AU', {
        "show_addresses": false,
        "show_locations": true,
        "location_params": {
        "location_types": "locality"
        },
        "max_results": "30"
        }
        );

        addressFinderInstance.on('location:select', function(fullLocation, metaData) {
        document.getElementById('ab_propertyname-1728354172346').value = metaData.full_location;
        suburbLocation = metaData.full_location;
        document.getElementById('address1_city-1722377736410').value = metaData.locality_name;
        document.getElementById('address1_stateorprovince-1722451231490').value = metaData.state_territory;
        document.getElementById('address1_postalcode-1722451251850').value = metaData.postcode;
        document.getElementById('address1_country-1722451278746').value = "Australia";
        if(suburbLocation != ""){
          if(suburbLocation != document.getElementById('ab_propertyname-1728354172346').value){
            document.getElementById("suburb-message").style.display = "block";
            document.getElementById('ab_propertyname-1728354172346').classList.add("icon-error");
          }
          else{
            document.getElementById("suburb-message").style.display = "none";
            document.getElementById('ab_propertyname-1728354172346').classList.remove("icon-error");
          }
        }
        });
        }

        // Function to inject AddressFinder script into the document
        function injectAddressFinderScript() {
        if (!document.getElementById('addressfinder-widget-script')) {
        var scriptTag = document.createElement('script');
        scriptTag.id = 'addressfinder-widget-script';
        scriptTag.src = 'https://api.addressfinder.io/assets/v3/widget.js';
        scriptTag.async = true;
        scriptTag.onload = initializeSuburbFinderWidget;
        document.body.appendChild(scriptTag);
        } else {
        //initializeSuburbFinderWidget();
        }
        if(suburbLocation != ""){
          if(suburbLocation != document.getElementById('ab_propertyname-1728354172346').value){
            document.getElementById("suburb-message").style.display = "block";
                document.getElementById('ab_propertyname-1728354172346').classList.add("icon-error");
          }
          else{
            document.getElementById("suburb-message").style.display = "none";
            document.getElementById('ab_propertyname-1728354172346').classList.remove("icon-error");
          }
        }
        }

        // Function to handle changes to the suburb input field
        function onSuburbFieldChange() {
        injectAddressFinderScript();
        }

function iOS() {
  return [
    'iPad Simulator',
    'iPhone Simulator',
    'iPod Simulator',
    'iPad',
    'iPhone',
    'iPod'
  ].includes(navigator.platform)
  // iPad on iOS 13 detection
  || (navigator.userAgent.includes("Mac") && "ontouchend" in document)
}

        if(iOS()){
            var stylesheet = document.styleSheets[0];
            stylesheet.insertRule("ul.af_list { left: 38px !important;}", 0);
        }

        // Function to initialize AddressFinder Email Widget
        function setupAddressFinderWidget() {
        var config = {
        // Update the property selectors using the ID attributes of your form fields
        addressSelector: "#emailaddress1-1669977007902",

        // Paste in your licence key
        licenceKey: "QGWLATPDXYUEV6983MKJ",

        // Customise the behaviour by adding advanced options
        rules: {
        unverified: {
        rule: "block",
        message: "This email address cannot be verified."
        }
        }
        };

        var emailInput = document.querySelector(config.addressSelector);
        if (emailInput) {
        // Load AddressFinder script if it hasn't been loaded yet
        if (!document.getElementById("addressfinder-email-script")) {
        var script = document.createElement("script");
        script.id = "addressfinder-email-script";
        script.src = "https://api.addressfinder.io/assets/email/v1/widget.js";
        script.async = true;
        script.onload = function() {
        new AddressFinder.Email.Widget(config.addressSelector, config.licenceKey, config);
        };
        document.body.appendChild(script);
        } else {
        // Re-initialize the widget if the script is already loaded
        //new AddressFinder.Email.Widget(config.addressSelector, config.licenceKey, config);
        }
        }
        }

        // Function to handle changes to the email input field
        function handleEmailInputChange() {
        setupAddressFinderWidget();
        }

        // Function to load the AddressFinder Phone script and initialize the widget
        function loadAddressfinderPhoneScript() {
        if (!document.getElementById("addressfinder-phone-script")) {
        var script = document.createElement("script");
        script.id = "addressfinder-phone-script";
        script.src = "https://api.addressfinder.io/assets/phone/v2/widget.js";
        script.async = true;
        script.onload = function() {

         setTimeout(() => {  
            initializeAddressfinderPhoneWidget();
           }, 1000);

       
        // CLONE NODE 
const observer = new MutationObserver(mutations => {
  mutations.forEach(mutation => {
    mutation.addedNodes.forEach(node => {
      if (node.nodeType !== 1) return; // Skip non-element nodes

      const newMessages = [];

      // Skip processing if it's already a custom message
      if (node.matches('.af-pv-message') && !node.classList.contains('is-custom')) {
        newMessages.push(node);
      }

      // Also check for children, but exclude already-custom ones
      const innerMessages = node.querySelectorAll('.af-pv-message:not(.is-custom)');
      innerMessages.forEach(msg => newMessages.push(msg));

      newMessages.forEach(original => {
        // Remove existing custom message
        const existingCustom = document.querySelector('.af-pv-message.is-custom');
        if (existingCustom) existingCustom.remove();

        // Clone and add .is-custom
        const clone = original.cloneNode(true);
        clone.classList.add('is-custom');

        // Append clone just before </body>
        document.body.appendChild(clone);

        // console.log('Updated .af-pv-message.is-custom:', clone);

        // Run opacity check after inserting
        updateCustomOpacity();
      });
    });
  });
});

observer.observe(document.body, {
  childList: true,
  subtree: false
});

// Function to update opacity based on presence of .af-pv-icon-success
const updateCustomOpacity = () => {
  const customMsg = document.querySelector('.af-pv-message.is-custom');
  const successIcon = document.querySelector('.af-pv-icon-success');

  if (customMsg) {
    customMsg.style.opacity = successIcon ? '0' : '1';
  }
};

// Observer to watch for appearance/removal of .af-pv-icon-success
const successObserver = new MutationObserver(() => {
  updateCustomOpacity();
});

successObserver.observe(document.body, {
  childList: true,
  subtree: true
});

// Run once initially in case success icon already exists
updateCustomOpacity();

