        // EMAIL VALIDATION START
let afEvMessageText = "";
const emailTextTarget = document.getElementById('email-af');

const emailMessageObserver = new MutationObserver(mutations => {
  mutations.forEach(mutation => {
    mutation.addedNodes.forEach(node => {
      if (node.nodeType !== 1) return;

      // Directly added message
      if (node.matches('.af-ev-message')) {
        afEvMessageText = node.textContent.trim();
        console.log("Email message added:", afEvMessageText);
        if (emailTextTarget) {
          emailTextTarget.textContent = afEvMessageText;
        }
      }

      // Messages inside added nodes
      const innerMessages = node.querySelectorAll?.('.af-ev-message');
      innerMessages?.forEach(msg => {
        afEvMessageText = msg.textContent.trim();
        console.log("Email message nested:", afEvMessageText);
        if (emailTextTarget) {
          emailTextTarget.textContent = afEvMessageText;
        }
      });
    });

    // mutation.removedNodes.forEach(node => {
    //   if (node.nodeType === 1 && node.matches('.af-ev-message')) {
    //     afEvMessageText = "";
    //     console.log("Email message removed.");
    //     if (emailTextTarget) {
    //       emailTextTarget.textContent = afEvMessageText;
    //     }
    //   }
    // });
  });
});

// Observe for added/removed .af-ev-message
emailMessageObserver.observe(document.body, {
  childList: true,
  subtree: true
});



// Create a MutationObserver to watch for added nodes and class changes
const observer = new MutationObserver(mutations => {
  mutations.forEach(mutation => {
    // Check if nodes have been added
    mutation.addedNodes.forEach(node => {
      // If the added node has the 'af-ev-icon-success' class
      if (node.nodeType === 1 && node.classList.contains('af-ev-icon-success')) {
        console.log('af-ev-icon-success class has been added 1');
        if (emailTextTarget) {
          emailTextTarget.textContent = "";
        }
      }

      // Also check for class changes on existing nodes
      if (node.nodeType === 1) {
        const addedClass = node.classList.contains('af-ev-icon-success');
        if (addedClass) {
          console.log('af-ev-icon-success class has been added 2');
          if (emailTextTarget) {
            emailTextTarget.textContent = "";
          }
        }
      }
    });
  });
});

// Configuration to watch for added nodes in the DOM
const config = { childList: true, subtree: true };

// Start observing the document body (or any specific element)
observer.observe(document.body, config);




