window.addEventListener('resize', () => {
  console.log("resized 3");
  var phoneInput = document.querySelector("#mobilephone-1722356712050");
var errorMessage = document.querySelector("span.af-pv-message.is-custom");
  const customMsg = document.querySelector('.af-pv-message.is-custom');
  if (customMsg) {
    // customMsg.style.opacity = '0'; 
    if (phoneInput && errorMessage) {
    var phoneInputRect = phoneInput.getBoundingClientRect();
    errorMessage.style.position = "absolute";
    errorMessage.style.top = (phoneInputRect.top + window.scrollY + 70) + "px";
    errorMessage.style.left = (phoneInputRect.left + window.scrollX) + "px";
}
    // Optionally add a delay to ensure styles are applied after resize
    setTimeout(() => {   
      //  customMsg.style.opacity = '0';
      if (phoneInput && errorMessage) {
    var phoneInputRect = phoneInput.getBoundingClientRect();
    errorMessage.style.position = "absolute";
    errorMessage.style.top = (phoneInputRect.top + window.scrollY + 70) + "px";
    errorMessage.style.left = (phoneInputRect.left + window.scrollX) + "px";
}
    }, 800);
  }
});




