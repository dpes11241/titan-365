        // function to set raw international field
          function setRawInternationalField(metadata){
            debugger;
            try {
              var rawIntNumb = document.querySelector('input[name="ab_mobilephone_raw_international"]');
              if(rawIntNumb && rawIntNumb.value && metadata.raw_international)
                rawIntNumb.value = metadata.raw_international
            } catch (error) {
              console.log(error);
            }
          }

        // Function to initialize the AddressFinder Phone Widget
        function initializeAddressfinderPhoneWidget() {
          console.log("initializeAddressfinderPhoneWidget 409");
        var phoneInput = document.querySelector("#mobilephone-1722356712050");
        if (phoneInput) {
        var afWidget = new AddressfinderPhone.Phone.Widget("#mobilephone-1722356712050", "QGWLATPDXYUEV6983MKJ", {
        defaultCountryCode: "AU",
        allowedCountryCodes: "AU",
        timeout: "10",
        rules: {
        unverified: {
        rule: "block",
        message: "This phone number cannot be verified."
        },
        nonMobile: {
        rule: "allow",
        message: "Incorrect format. If entering a landline, include the area code."
        },
        countryNotAllowed: {
        rule: "allow",
        message: "Please provide an Australian mobile phone number."
        }
        }
        });

        afWidget.on("result:verified", function (metadata) {
            setRawInternationalField(metadata);
             console.log("result:verified 434");
          });

          afWidget.on("result:not_verified", function (metadata) {
            setRawInternationalField(metadata);
             console.log("result:not_verified 439");
               setTimeout(() => {  
                const customMessageSpan = document.querySelector("span.af-pv-message.is-custom");
                if (customMessageSpan) {
                  customMessageSpan.textContent = "Incorrect format. If entering a landline, include the area code.";
                }
            }, 1000);
             
          });

        }
        }

        // Function to handle changes to the phone number input field
        function onPhoneNumberChange() {
        loadAddressfinderPhoneScript();
        // console.log("onPhoneNumberChange triggered 10");
        }

        // Wait for the DOM to load before attaching event listeners
        document.addEventListener("d365mkt-afterformload", function() {
        // Initialize phone field change event listener
        var phoneInput = document.querySelector("#mobilephone-1722356712050");
        if (phoneInput) {
        phoneInput.addEventListener("focus", onPhoneNumberChange);
        phoneInput.addEventListener("select", onPhoneNumberChange);
        phoneInput.addEventListener("input", onPhoneNumberChange);
        phoneInput.addEventListener("change", onPhoneNumberChange);
        phoneInput.addEventListener("paste", onPhoneNumberChange);
        phoneInput.addEventListener("blur", onPhoneNumberChange);
        }

        // Initialize email field change event listener
        var emailInput = document.querySelector("#emailaddress1-1669977007902");
        if (emailInput) {
        emailInput.addEventListener("focus", handleEmailInputChange);
        emailInput.addEventListener("select", handleEmailInputChange);
        emailInput.addEventListener("input", handleEmailInputChange);
        emailInput.addEventListener("change", handleEmailInputChange);
        emailInput.addEventListener("paste", handleEmailInputChange);
        emailInput.addEventListener("blur", handleEmailInputChange);
        }

        var suburbInput = document.querySelector('#ab_propertyname-1728354172346');
        if (suburbInput) {
        suburbInput.addEventListener("focus", onSuburbFieldChange);
